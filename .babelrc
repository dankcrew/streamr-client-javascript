{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "usage",
            "modules": false,
            "corejs": 3,
            "loose": false,
            "targets": {
                "esmodules": true
            },
            "exclude": ["transform-regenerator", "@babel/plugin-transform-regenerator"]
        }]
    ],
    "plugins": [
        "add-module-exports",
        ["@babel/plugin-transform-runtime", {
            "corejs": false,
            "helpers": true,
            "regenerator": false
        }],
        "@babel/plugin-transform-modules-commonjs",
        ["@babel/plugin-proposal-class-properties", {
            "loose": false
        }]
    ]
}
